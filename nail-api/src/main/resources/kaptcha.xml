<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>Google Kaptcha</comment>
 
	<entry key="kaptcha.border">yes</entry>
	<entry key="kaptcha.border.color">105,179,90</entry>
	<entry key="kaptcha.border.thickness">1</entry>
	<!-- <entry key="kaptcha.producer.impl">com.google.code.kaptcha.impl.DefaultKaptcha</entry>
	<entry key="kaptcha.textproducer.impl">com.google.code.kaptcha.text.impl.DefaultTextCreator</entry>
	<entry key="kaptcha.textproducer.char.string">abcde2345678gfynmnpwx</entry> -->
	<entry key="kaptcha.textproducer.font.color">BLUE</entry>
	<entry key="kaptcha.textproducer.font.size">45</entry>
	<entry key="kaptcha.textproducer.char.length">4</entry>
	<entry key="kaptcha.textproducer.font.names">宋體,楷體,微軟雅黑</entry>
	<entry key="kaptcha.textproducer.char.space">2</entry>
	<!-- <entry key="kaptcha.noise.impl">com.google.code.kaptcha.impl.DefaultNoise</entry>
	<entry key="kaptcha.noise.color">BLACK</entry>
	<entry key="kaptcha.obscurificator.impl">com.google.code.kaptcha.impl.WaterRipple</entry>
	<entry key="kaptcha.word.impl">com.google.code.kaptcha.text.impl.DefaultWordRenderer</entry>
	<entry key="kaptcha.background.impl">com.google.code.kaptcha.impl.DefaultBackground</entry>
	<entry key="kaptcha.background.clear.from">LIGHT_GRAY</entry>
	<entry key="kaptcha.background.clear.to">WHITE</entry> -->
	<entry key="kaptcha.image.width">200</entry>
	<entry key="kaptcha.image.height">45</entry>
	<entry key="kaptcha.session.key">code</entry>
	<entry key="kaptcha.session.date">KAPTCHA_SESSION_DATE</entry>
</properties>